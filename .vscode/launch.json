{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "cpp Local Attach",
            "type": "cppdbg",
            "request": "attach",
            "processId":"${command:pickProcess}",
            "program": "${workspaceFolder}/objs/nginx",
            "MIMode": "gdb",
            "logging": {
                "engineLogging": true,
                "traceResponse": true
            },
            // "stopAtEntry":true,
            "sourceFileMap": {
                "/home/learning/nginx/src": "/home/learning/nginx/src"
            },
            "setupCommands": [{
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }],
        },
        {
            "name": "(gdb) 远程调试ubuntu",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/objs/nginx",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "linux": {
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/gdb",
                "miDebuggerServerAddress": "127.0.0.1:2333"
            },
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb"
        }
    ]
}